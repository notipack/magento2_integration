{
  "name":"notipack/magento2_integration",
  "description":"This module will automatically add notifications from notipack to your site and send the information needed to create them.",
  "type":"magento2-module",
  "version":"1.0.2",
  "license":[
    "OSL-3.0",
    "AFL-3.0"
  ],
  "require":{
    "php":">=5.5",
    "magento/framework":"100.*|101.*|102.*|103.*|104.*",
    "magento/module-sales":"100.*|101.*|102.*|103.*|104.*",
    "ext-curl":"*"
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://repo.magento.com/"
    }
  ],
  "autoload":{
    "files":[
      "registration.php"
    ],
    "psr-4":{
      "Notipack\\Integration\\":""
    }
  }
}
